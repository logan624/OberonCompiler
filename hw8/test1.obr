MODULE Main;
VAR  a, b, c: INTEGER;
PROCEDURE FirstProcedure;
VAR  a, x: INTEGER;
BEGIN
  a := 10;
  x := a + 5;
END FirstProcedure;

PROCEDURE SecondProcedure;
VAR  b, y: INTEGER;
BEGIN
  b := 20;
  y := b * 2;
END SecondProcedure;

PROCEDURE ThirdProcedure;
VAR
  c, z: INTEGER;
BEGIN
  c := 30;
  z := c - 5;
END ThirdProcedure;

BEGIN
  a := 1;
  b := 2;
  c := 3;
  FirstProcedure();
  SecondProcedure();
  ThirdProcedure();
END Main.
